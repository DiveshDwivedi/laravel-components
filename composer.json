{
    "name": "mobtexting/laravel-components",
    "description": "Blade components.",
    "keywords": [],
    "license": "MIT",
    "type": "library",
    "authors": [],
    "require": {
        "php": "^8.0|^8.1",
        "protonemedia/laravel-form-components": "^3.6"
    },
    "require-dev": {
        "livewire/livewire": "^2.0",
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench-browser-kit": "^6.23",
        "spatie/laravel-translatable": "^4.4",
        "symfony/dom-crawler": "^5.1.14",
        "diviky/code-analysis": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Mobtexting\\LaravelComponents\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mobtexting\\LaravelComponents\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpro/grumphp": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Mobtexting\\LaravelComponents\\Support\\ServiceProvider"
            ]
        }
    }
}
